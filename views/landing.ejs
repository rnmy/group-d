<!DOCTYPE html>
<html>
    <head>
        <title>Group'd</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/css/bootstrap-select.min.css">
        <link rel="stylesheet" href="/stylesheets/styles.css">
        <link rel="stylesheet" href="/stylesheets/search-filter.css">
        <link href="https://fonts.googleapis.com/css2?family=Pacifico&display=swap" rel="stylesheet">
    </head>
    <body>
        <div class="container mt-3">
            <% if(error && error.length > 0){ %>
                <div class="alert alert-danger" role="alert">
                    <%= error %>
                </div>
            <% } %>
            <% if(success && success.length > 0){ %>
                <div class="alert alert-success" role="alert">
                    <%= success %>
                </div>
            <% } %>
        </div>

      <div class="container" id="landing-header">
        <h1 id="landingPageTitle" class="display-4 text-center">
          group'd 
        </h1>
      </div>
      <div id="landingPageButtons">
        <div class="row">
          <div class="col text-center form-inline">
            <div class="form-group mr-5">
              <a href="/login"><button class="landingPageButton btn btn-light btn-outline-dark form-control text-dark" style="width: 100px;">Login</button></a>
            </div>
            <div class="form-group">
              <a href="/register"><button class="landingPageButton btn btn-light btn-outline-dark form-control text-dark" style="width: 100px;">Register</button></a>
            </div>
          </div>
        </div>
      </div>
  <ul class="slideshow">
    <li></li>
    <li></li>
    <li></li>
    <li></li>
    <li></li>
  </ul>


